#include<stdio.h>
#include<stdlib.h>

struct strings
{
    char string;
};

struct str
{
    struct strings *a;
    int n;
    
}

struct str input()
{
    struct str f;
    int i;
    printf("Enter The Number Of Words\n");
    scanf("%d",&f.n);
    f.a=malloc(f.n*sizeof(struct strings));
    for(i=0;i<f.n;i++)
       {
           printf("Enter Word %d\n",i+1);
           gets(f.a[i].string);
        }
    return f;    
    
    
}

int main()
{
    f=input();
    f=compute(f);
    output f;
    return 0;
}
